<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tyre_repairer_favorite_action" model="ir.actions.act_window">
        <field name="name">Tyre repairer Favorite</field>
        <field name="res_model">tyre.repairer.favorite</field>
        <field name="view_mode">tree</field>
    </record>
    <menuitem
        id="tyre_repairer_favorite_menu"
        name="Tyre repairer favorite"
        action="tyre_repairer_favorite_action"
        parent="bloomup_fleet_move.fleet_settigs_menu"
        sequence="12"/>

    <record id="tyre_repairer_favorite_view_tree" model="ir.ui.view">
        <field name="name">tyre.repairer.favorite.view.tree</field>
        <field name="model">tyre.repairer.favorite</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="customer_code" />
                <field name="customer_name" />
                <field name="customer_id" />
                <field name="tyre_repairer_code" />
                <field name="tyre_repairer_name" />
            </tree>
        </field>
    </record>
</odoo>